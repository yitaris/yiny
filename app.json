{
  "expo": {
    "name": "ubgida_01",
    "slug": "ubgida_01",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/ubgidalight.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yigit.ubgida",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ubgidalight.png",
        "backgroundColor": "#121212"
      },
      "package": "com.yitaris.ubgida_01"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ubgidalight.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ubgidalight.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#121212"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "975b058c-57c9-4225-a505-6cb41a7ec32e"
      }
    },
    "owner": "yitaris"
  }
}
